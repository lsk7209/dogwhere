# 클라우드플레어 Pages 환경변수 설정
# 이 파일은 .env.local로 복사하여 사용하세요

# API 키 설정
GOOGLE_PLACES_KEY=your_google_places_api_key_here
KAKAO_API_KEY=your_kakao_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# 내부 토큰
INTERNAL_TOKEN=your_internal_token_here

# 클라우드플레어 설정
CF_KV_NAMESPACE=your_kv_namespace_id
CF_D1_BINDING=your_d1_database_id
CF_R2_BUCKET=your_r2_bucket_name

# 슬랙 웹훅 (선택사항)
SLACK_WEBHOOK_URL=your_slack_webhook_url

# 공개 API URL
NEXT_PUBLIC_API_URL=https://your-domain.pages.dev

# 개발 환경 설정
NODE_ENV=production
